{
    "version": 2,
    "name": "nodejs-express",
    "builds": [
      { "src": "index.js", "use": "@now/node-server"}
    ],
    "routes": [
      {
        "src": "/*",
        "methods": ["GET", "POST", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept",
          "Access-Control-Allow-Credentials": "true"
        },
        "dest": "index.js",
        "continue": true
      },
      { "src": "/(.*)", "dest": "index.js"}
    ]
  }